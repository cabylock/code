git init : creat a repository

git clone + link_name  : clone from github

git branch : show all branch

git branch + branch_name : create a new branch

git checkout + branch_name : switch to branch

git checkout -b + branch_name : create and switch to branch

git brach -d + branch_name : delete branch

git add + file_name : add file to stage

git commit -m "message" : commit file to local repository

git add . : add all file to stage

git commit -am "message" : add all file to stage and commit

git push : push to github
git pull : pull from github

git merge + branch_name : merge branch to current branch

git rebash + branch_name : rebase branch to current branch

git log : show commit history

git reset : reset to last commit

git reset --hard + commit_id : reset to commit_id

git reset --soft + commit_id : reset to commit_id and keep file in stage

git reset --mixed + commit_id : reset to commit_id and keep file in working directory

git reset HEAD + file_name : remove file from stage

git ammend : edit last commit

git fetch : fetch from github






